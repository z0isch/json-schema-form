{
  "name": "json-schema-form",
  "version": "0.1.0",
  "description": "A web component that generates HTML forms from JSON Schema documents",
  "type": "module",
  "main": "dist/json-schema-form.js",
  "module": "dist/json-schema-form.js",
  "types": "dist/json-schema-form.d.ts",
  "exports": {
    ".": {
      "types": "./dist/json-schema-form.d.ts",
      "import": "./dist/json-schema-form.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
