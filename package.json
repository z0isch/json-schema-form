  },
  "keywords": [
    "json-schema",
    "form",
    "web-component",
    "lit",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "typescript": "^5.3.3",
    "vite": "^5.4.2"
  },
  "dependencies": {
    "lit": "^3.1.2"
  }
